{
  "expo": {
    "name": "freeshow-companion",
    "slug": "freeshow-companion",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E91E63"
    },
    "ios": {
      "supportsTablet": true,
      "orientation": "default"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E91E63"
      },
      "edgeToEdgeEnabled": true,
      "orientation": "default",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.gladson1205.freeshowcompanion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-barcode-scanner"
    ],
    "extra": {
      "eas": {
        "projectId": "7aa212fb-ba21-43f4-92d3-83f772a2f2a8"
      }
    },
    "owner": "gladson1205"
  }
}
